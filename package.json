  "devDependencies": {
    "@types/del": "3.0.1",
    "@types/glob": "5.0.35",
    "@types/jest": "21.1.10",
    "@types/lodash": "4.14.109",
    "@types/make-dir": "1.0.3",
    "@types/nock": "9.1.3",
    "@types/node": "8.10.17",
    "@types/request": "2.47.0",
    "@types/rev-hash": "2.0.0",
    "del": "3.0.0",
    "jest": "21.2.1",
    "prettier": "1.13.2",
    "prettier-config-ikatyang": "1.1.1",
    "request": "2.87.0",
    "standard-version": "4.4.0",
    "ts-jest": "21.2.4",
    "tslint": "5.10.0",
    "tslint-config-ikatyang": "2.5.1",
    "tslint-config-prettier": "1.13.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.6.2"
  },
  "peerDependencies": {
    "jest": "^20.0.0 || ^21.0.0"
